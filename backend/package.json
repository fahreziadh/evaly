    "@elysiajs/eden": "^1.2.0",
    "better-auth": "^1.2.2",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "elysia": "latest"
  },
  "devDependencies": {
    "@types/bun": "^1.2.4",
    "bun-types": "latest"
  },
  "module": "src/index.js"
}