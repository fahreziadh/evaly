{
  "name": "@evaly/backend",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "prepare": "bun tsup src/index.ts src/types/*.ts --dts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ENVIRONMENT=development bun run --watch src/index.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "bunx drizzle-kit generate --dialect postgresql --schema ./src/lib/db/schema --out ./drizzle",
    "db:migrate": "bun db:generate && bun run src/lib/db/migrate.ts && bun run prepare",
    "db:push": "bunx drizzle-kit push",
    "check-types": "tsc --noEmit",
    "email": "email dev --dir src/emails",
    "trigger:dev": "npx trigger.dev@latest dev",
    "trigger:login": "npx trigger.dev@latest login --profile default -a https://trigger.evaly.io",
    "trigger:logout": "npx trigger.dev@latest logout --profile default",
    "trigger:deploy:staging": "npx trigger.dev deploy --env staging -a https://trigger.evaly.io --self-hosted"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.0",
    "@aws-sdk/client-rds-data": "^3.758.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/eden": "^1.2.0",
    "@elysiajs/opentelemetry": "1.2.0",
    "@elysiajs/swagger": "^1.2.2",
    "@react-email/components": "^0.0.33",
    "@sinclair/typebox": "^0.34.28",
    "@trigger.dev/sdk": "^3.3.17",
    "ai": "^4.2.0",
    "better-auth": "1.2.4",
    "bun": "1.2.5",
    "dotenv": "^16.4.7",
    "drizzle-kit": "0.30.5",
    "drizzle-orm": "0.40.0",
    "drizzle-typebox": "^0.3.0",
    "elysia": "1.2.25",
    "nodemailer": "^6.10.0",
    "openai": "^4.87.3",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@trigger.dev/build": "^3.3.17",
    "@types/bun": "1.2.5",
    "@types/nodemailer": "^6.4.17",
    "bun-types": "latest",
    "react-email": "^3.0.7",
    "tsup": "^8.4.0"
  },
  "overrides": {
    "@sinclair/typebox": "^0.34.27"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    }
  },
  "type": "module"
}
