{
  "name": "@evaly/backend",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "bun run src/lib/db/migrate.ts",
