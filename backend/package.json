{
  "name": "@evaly/backend",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "prepare": "bun tsup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ENVIRONMENT=development bun run --watch src/index.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "bunx drizzle-kit generate --dialect postgresql --schema ./src/lib/db/schema --out ./drizzle",
    "db:migrate": "bun db:generate && ENVIRONMENT=development bun drizzle-kit migrate && ENVIRONMENT=staging bun drizzle-kit migrate && bun run prepare",
    "db:push": "bunx drizzle-kit push",
    "check-types": "tsc --noEmit",
    "email": "email dev --dir src/emails",
    "trigger:dev": "bunx trigger.dev@latest dev",
    "trigger:deploy": "bunx trigger.dev@latest deploy --self-hosted --env prod --push"
  },
  "dependencies": {
    "@ai-sdk/google": "1.2.3",
    "@ai-sdk/openai": "1.3.2",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/eden": "^1.2.0",
    "@elysiajs/opentelemetry": "1.2.0",
    "@elysiajs/swagger": "^1.2.2",
    "@mem0/vercel-ai-provider": "^0.0.14",
    "@react-email/components": "^0.0.33",
    "@sinclair/typebox": "^0.34.28",
    "@trigger.dev/sdk": "3.3.17",
    "ai": "4.2.5",
    "better-auth": "1.2.4",
    "dotenv": "^16.4.7",
    "drizzle-kit": "0.30.6",
    "drizzle-orm": "0.41.0",
    "drizzle-typebox": "0.3.1",
    "elysia": "1.2.25",
    "elysia-compress": "^1.2.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.10.0",
    "openai": "4.89.0",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "1.2.5",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "bun": "1.2.8",
    "bun-types": "latest",
    "react-email": "^3.0.7",
    "tsup": "^8.4.0"
  },
  "overrides": {
    "@sinclair/typebox": "^0.34.27"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    }
  },
  "type": "module"
}